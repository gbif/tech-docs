#. type: Plain text
#: en/data-use/modules/ROOT/pages/rgbif.adoc:4
msgid "_An interface to the GBIF API for the R statistical programming environment._"
msgstr ""

#. type: Plain text
#: en/data-use/modules/ROOT/pages/rgbif.adoc:6
msgid "Please see the main https://docs.ropensci.org/rgbif/index.html[rgbif documentation site]."
msgstr ""

#. type: Plain text
#: en/data-use/modules/ROOT/pages/rgbif.adoc:8
#, no-wrap
msgid "**rgbif** is an R package from https://ropensci.org/[rOpenSci] that allows searching and retrieving data from GBIF. rgbif wraps R code around the xref:api-introduction.adoc[GBIF API] to allow you to talk to GBIF from R and access metadata, species names, and occurrences.\n"
msgstr ""

#. type: delimited block -
#: en/data-use/modules/ROOT/pages/rgbif.adoc:15
#, no-wrap
msgid "install.packages(\"rgbif\") # CRAN version\n"
msgstr ""

#. type: delimited block -
#: en/data-use/modules/ROOT/pages/rgbif.adoc:20
#, no-wrap
msgid "pak::pkg_install(\"ropensci/rgbif\") # dev version\n"
msgstr ""

#. type: delimited block -
#: en/data-use/modules/ROOT/pages/rgbif.adoc:25
#, no-wrap
msgid "install.packages(\"rgbif\", repos=\"https://dev.ropensci.org\") # dev version\n"
msgstr ""

